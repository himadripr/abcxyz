<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.baseinfotech.juscep">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="sendotp.key"
            android:value="@string/sendotp_key" />

        <activity
            android:name=".activity.LaunchingActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:screenOrientation="nosensor"
            android:theme="@style/FullscreenTheme" />
        <activity
            android:name=".activity.LaunchingActivity2"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.AskPermissionActivity"
            android:label="@string/app_name"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.RegisterActivity"
            android:label="@string/app_name"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".activity.BookingActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.MyAccountActivity" />
        <activity android:name=".activity.GalleryActivity" />
        <activity android:name=".activity.AboutUsActivity" />
        <activity android:name=".activity.FAQActivity" />
        <activity android:name=".activity.PrivacyPolicyActivity" />
        <activity android:name=".activity.ContactUsActivity" />
        <activity android:name=".activity.ChangePasswordActivity"></activity>
    </application>

</manifest>